name: dockercompose4566201646795642128
services:
  poc.api:
    build:
      context: /Users/jefferson/Projects/POC
      dockerfile: POC/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: POC.API
      target: base
    container_name: POC.API
    entrypoint:
      - tail
      - -f
      - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: ""
    image: poc:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net7.0/POC.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        protocol: tcp
      - mode: ingress
        target: 443
        protocol: tcp
    tty: true
    volumes:
      - type: bind
        source: /Users/jefferson/.aspnet/https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: /Users/jefferson/.microsoft/usersecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: /Users/jefferson/Projects/POC/POC
        target: /app
        bind:
          create_host_path: true
      - type: bind
        source: /Users/jefferson/Projects/POC
        target: /src
        bind:
          create_host_path: true
      - type: bind
        source: /Users/jefferson/vsdbg/vs2017u5
        target: /remote_debugger
        bind:
          create_host_path: true
      - type: bind
        source: /Users/jefferson/.nuget/packages/
        target: /root/.nuget/packages
        read_only: true
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose4566201646795642128_default